{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@open-press/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@open-press/config": "<rootDir>/../libs/config/src",
    "@open-press/example-plugin/(.*)": "<rootDir>/../libs/example-plugin/src/$1",
    "@open-press/example-plugin": "<rootDir>/../libs/example-plugin/src",
    "@open-press/models/(.*)": "<rootDir>/../libs/models/src/$1",
    "@open-press/models": "<rootDir>/../libs/models/src",
    "@open-press/support/(.*)": "<rootDir>/../libs/support/src/$1",
    "@open-press/support": "<rootDir>/../libs/support/src",
    "@open-press/utility/(.*)": "<rootDir>/../libs/utility/src/$1",
    "@open-press/utility": "<rootDir>/../libs/utility/src",
    "@open-press/backend-interfaces/(.*)": "<rootDir>/../libs/interfaces/src/$1",
    "@open-press/backend-interfaces": "<rootDir>/../libs/interfaces/src"
  }
}