{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/aetheria-backend/src",
	"compilerOptions": {
		"deleteOutDir": false,
		"webpack": false,
		"tsConfigPath": "apps/aetheria-backend/tsconfig.app.json"
	},
	"projects": {
		"config": {
			"type": "library",
			"root": "libs/config",
			"entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "models": {
      "type": "library",
      "root": "libs/models",
      "entryFile": "index",
      "sourceRoot": "libs/models/src",
      "compilerOptions": {
        "tsConfigPath": "libs/models/tsconfig.lib.json"
      }
    },
    "support": {
      "type": "library",
      "root": "libs/support",
      "entryFile": "index",
      "sourceRoot": "libs/support/src",
      "compilerOptions": {
        "tsConfigPath": "libs/support/tsconfig.lib.json"
      }
    },
    "utility": {
      "type": "library",
      "root": "libs/utility",
      "entryFile": "index",
      "sourceRoot": "libs/utility/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utility/tsconfig.lib.json"
      }
    },
    "interfaces": {
      "type": "library",
      "root": "libs/interfaces",
      "entryFile": "index",
      "sourceRoot": "libs/interfaces/src",
      "compilerOptions": {
        "tsConfigPath": "libs/interfaces/tsconfig.lib.json"
      }
    },
		"aetheria-backend": {
			"type": "application",
			"root": "apps/aetheria-backend",
			"entryFile": "main",
			"sourceRoot": "apps/aetheria-backend/src",
			"compilerOptions": {
				"tsConfigPath": "apps/aetheria-backend/tsconfig.app.json"
			}
		},
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    },
    "plugin-example": {
      "type": "library",
      "root": "libs/plugin-example",
      "entryFile": "index",
      "sourceRoot": "libs/plugin-example/src",
      "compilerOptions": {
        "tsConfigPath": "libs/plugin-example/tsconfig.lib.json"
      }
    },
    "plugin-tailwind-classes-builder": {
      "type": "library",
      "root": "libs/plugin-tailwind-classes-builder",
      "entryFile": "index",
      "sourceRoot": "libs/plugin-tailwind-classes-builder/src",
      "compilerOptions": {
        "tsConfigPath": "libs/plugin-tailwind-classes-builder/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
	"root": "apps/aetheria-backend"
}
