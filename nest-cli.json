{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/open-press-backend/src",
  "compilerOptions": {
    "deleteOutDir": false,
    "webpack": false,
    "tsConfigPath": "apps/open-press-backend/tsconfig.app.json"
  },
  "projects": {
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "example-plugin": {
      "type": "library",
      "root": "libs/example-plugin",
      "entryFile": "index",
      "sourceRoot": "libs/example-plugin/src",
      "compilerOptions": {
        "tsConfigPath": "libs/example-plugin/tsconfig.lib.json"
      }
    },
    "models": {
      "type": "library",
      "root": "libs/models",
      "entryFile": "index",
      "sourceRoot": "libs/models/src",
      "compilerOptions": {
        "tsConfigPath": "libs/models/tsconfig.lib.json"
      }
    },
    "support": {
      "type": "library",
      "root": "libs/support",
      "entryFile": "index",
      "sourceRoot": "libs/support/src",
      "compilerOptions": {
        "tsConfigPath": "libs/support/tsconfig.lib.json"
      }
    },
    "utility": {
      "type": "library",
      "root": "libs/utility",
      "entryFile": "index",
      "sourceRoot": "libs/utility/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utility/tsconfig.lib.json"
      }
    },
    "interfaces": {
      "type": "library",
      "root": "libs/interfaces",
      "entryFile": "index",
      "sourceRoot": "libs/interfaces/src",
      "compilerOptions": {
        "tsConfigPath": "libs/interfaces/tsconfig.lib.json"
      }
    },
    "open-press-backend": {
      "type": "application",
      "root": "apps/open-press-backend",
      "entryFile": "main",
      "sourceRoot": "apps/open-press-backend/src",
      "compilerOptions": {
        "tsConfigPath": "apps/open-press-backend/tsconfig.app.json"
      }
    },
    "cli": {
      "type": "application",
      "root": "apps/cli",
      "entryFile": "main",
      "sourceRoot": "apps/cli/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cli/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/open-press-backend"
}